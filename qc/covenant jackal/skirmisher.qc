$include "kigyar_shared.qci"
$scale 1
$modelname	"hce/spv3/cov/Skirmisher/Skirmisher.mdl"
$body Skirmisher "Skirmisher.smd"
$cdmaterials "models/hce/spv3/cov/Skirmisher"
$surfaceprop "flesh"
$bodygroup "Shield"
{
	blank
	studio "Skirmisher_shield.smd"
}
$bodygroup "Head"
{
	studio "Skirmisher_Head1.smd"
	studio "Skirmisher_Head2.smd"
}

$animation "wF" "skirmisher_anims\wF.smd" {
	fps 30
	ikrule l_foot footstep range .. .. .. ..
	ikrule r_foot footstep range .. .. .. ..
	walkframe 30 LX LY
	rotate -90
}

$animation "EvadeL" "skirmisher_anims\EvadeL.smd" {
	fps 30
	walkframe 44 LX LY
	rotate -90
}

$animation "EvadeR" "skirmisher_anims\EvadeR.smd" {
	fps 30
	walkframe 49 LX LY
	rotate -90
}

$animation "rif_f" "skirmisher_anims/Rif_moveF" {
	fps 30
	loop
	walkframe 60 LX LY
	rotate -90
}
$animation "rif_b" "jackal_anims/Rif_moveB" {
	fps 60
	loop
	walkframe 60 LX LY
	rotate -90
}
$animation "rif_r" "jackal_anims/Rif_moveL" {
	fps 60
	loop
	walkframe 60 LX LY
	rotate -90
}
$animation "rif_l" "jackal_anims/Rif_moveR" {
	fps 60
	loop
	walkframe 60 LX LY
	rotate -90
}

$sequence "Rifle_run" {
	"rif_b"
	"rif_r"
	"rif_f"
	"rif_l"
	"rif_b"
	{ event 1100 2 "Step" }
	{ event 1100 8 "Step" }
	{ event 1100 16 "Step" }
	{ event 1100 21 "Step" }
	blend "move_yaw" -180 180
	blendwidth 5
	activity "ACT_RUN_RIFLE" 1
	fadein 0.2
	fadeout 0.2

}

$sequence "Run" {
	"wB"
	"wR"
	"wF"
	"wL"
	"wB"
	activity "ACT_RUN" 1
	blend "move_yaw" -180 180
	blendwidth 5
	loop
	{ event 1100 1 "Step" }
	{ event 1100 12 "Step" }
	fadein 0.2
	fadeout 0.2
}

$sequence "EvadeLeft" {
	"EvadeL"
	activity "ACT_SIGNAL1" 1
	blend "move_yaw" -180 180
	{ event 1100 0 "EvadeL" }
	fadein 0.2
	fadeout 0.2
}

$sequence "EvadeRight" {
	"EvadeR"
	activity "ACT_SIGNAL2" 1
	blend "move_yaw" -180 180
	{ event 1100 0 "EvadeR" }
	fadein 0.2
	fadeout 0.2
}

