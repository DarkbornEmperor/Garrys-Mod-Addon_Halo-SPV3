$bodygroup "empty"
{
	blank
}
$bodygroup "Body"
{
	studio "Body.smd"
	studio "Body_flood.smd"
}
$bodygroup "Weapon"
{
	blank
	studio "Flood_Weapon.smd"
}
$sequence "Melee" {
	"melee.smd"
	activity "ACT_MELEE_ATTACK1" 1
	fadein 0.2
	fadeout 0.2
	walkframe 60 LX LY 
}
$sequence "Run" {
	"move_f.smd"
 	activity "ACT_RUN" -1 
 // 	ikrule l_foot footstep range .. 7 20 ..
	// ikrule r_foot footstep range .. 20 10 ..
	loop fps 30
	walkframe 60 LX LY 
	{ event 1100 1 "Step" }
	{ event 1100 8 "Step" }

}
$sequence "Airborne" {
	"Airborne.smd"
	activity "ACT_JUMP" 1
	loop
	fadein 0.2
	fadeout 0.2
}
$sequence "Idle" {
	"Idle.smd"  
	// cmdlist lockfeet
	activity ACT_IDLE -1 
	loop 
	fps 30
}

$sequence "Hit_Front" {
	"hit_f.smd"  
	fps 30
}

$sequence "Die_Front" {
	"kill_f.smd"  
	activity ACT_DIE_SIMPLE -1
	fps 30
}

$sequence "Die_Left" {
	"kill_l.smd"  
	activity ACT_DIE_SIMPLE -1
	fps 30
}

$sequence "Die_Right" {
	"kill_r.smd"  
	activity ACT_DIE_SIMPLE -1
	fps 30
}

$animation "death_airborne" "Die_Airborne.SMD" {
	fps 30
	loop
}

$sequence "Die_Airborne" {
	"death_airborne"
	loop
}

$collisionjoints "Physics.smd"
{
	$mass 90
	$inertia 10
	$damping 0.01
	$rotdamping 1.5

	$jointconstrain "frame head" x limit -20 40 0
	$jointconstrain "frame head" y limit -20 40 0
	$jointconstrain "frame head" z limit -70 40 0

	$jointconstrain "frame tailbase" x limit -20 40 0
	$jointconstrain "frame tailbase" y limit -20 40 0
	$jointconstrain "frame tailbase" z limit -70 40 0

	$jointconstrain "frame leftshoulder" x limit -20 40 0
	$jointconstrain "frame leftshoulder" y limit -20 40 0
	$jointconstrain "frame leftshoulder" z limit -70 40 0

	$jointconstrain "frame rightshoulder" x limit -20 40 0
	$jointconstrain "frame rightshoulder" y limit -20 40 0
	$jointconstrain "frame rightshoulder" z limit -70 40 0
}

